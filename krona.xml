    trocou_base = true;
    controle_loop = 0;
   }//if
   controle_loop++;
  }//wh
  
  //topo
  if(document.getElementById('img_jogador_top')!=null){
   document.getElementById('img_jogador_top').src = str_img_topo;
   document.getElementById('img_jogador_top').style.visibility = "visible";
  }
  //bottom/base, lembrando q intRandomMain foi setado por ultimo em bottom/base
  if(document.getElementById('img_jogador_bottom')!=null){
   document.getElementById('img_jogador_bottom').src = str_img_bottom;
   document.getElementById('img_jogador_bottom').alt = arr_atleta_nome[intRandomMain];
   document.getElementById('img_jogador_bottom').style.visibility = "visible";
  }
  if(document.getElementById('str_nome_jogador_bottom')!=null)document.getElementById('str_nome_jogador_bottom').innerHTML = arr_atleta_nome[intRandomMain];
  
  setTimeout('trocaImagemAtleta()',5000);
 }//fnc
