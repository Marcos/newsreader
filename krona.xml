<script>
 //=======================================
 function mudaAba(qual){
  $('#li_aba_noticias').removeClass('clicado');
  $('#li_aba_videos').removeClass('clicado');
  $('#div_aba_noticias').hide();
  $('#div_aba_videos').hide();
  $('#li_aba_'+qual).addClass('clicado');
  $('#div_aba_'+qual).show();
  if(qual=="videos"){
   $('#h1_titulo_area').html('V&iacute;deos');
  }else{
   $('#h1_titulo_area').html('Not&iacute;cias');
  }
 }//fnc
 //=======================================
 function loadConteudo(id_qual,qual){
  var agora = getNowString();
  $("#div_carregando_"+qual).show();
  $.ajax({
   url: 'ajax_noticias_dtl.php?area='+qual+'&id='+id_qual+'&now='+agora,
     success: function(data) {
    $('#div_conteudo_'+qual).html(data);
    $("#div_carregando_"+qual).hide();
     },
     error: function(){
    $("#div_carregando_"+qual).hide();
   }
  });
 }//fnc
 //=======================================
 function paginacao(pagina,qual){
  var agora = getNowString();
  $("#div_carregando_"+qual).show();
  $.ajax({
   url: 'ajax_noticias_lst.php?area='+qual+'&page='+pagina+'&now='+agora,
     success: function(data) {
    $('#div_list_'+qual).html(data);
    $("#div_carregando_"+qual).hide();
    $("#boxPaginacao_"+qual).html($("#boxPaginacao_hidden_"+qual+"_"+agora).html());
     },
     error: function(){
    $("#div_carregando_"+qual).hide();
   }
  });
 }
 //=======================================
</script>&#13;
