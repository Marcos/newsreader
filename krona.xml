    //alert(arr_atleta_foto_topo[intRandomMain][intRandomSub]);
    str_img_topo = diretorio+''+arr_atleta_foto_topo[intRandomMain][intRandomSub];
    trocou_topo = true;
    controle_loop = 0;
   }//if
   controle_loop++;
  }//wh
  
  while( (!trocou_base) && (controle_loop < 200) ){
   intRandomMain = rand(1,7) - 1;
   if(arr_atleta_foto_base[intRandomMain].length > 0){
    intRandomSub = rand(1,arr_atleta_foto_base[intRandomMain].length) - 1;
