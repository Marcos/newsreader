     arr_atleta_foto_base[0] = Array();
     arr_atleta_foto_base[0][0] = 'dudu_botom.png';
          arr_atleta_nome[1] = 'Leandrinho';
     arr_atleta_foto_topo[1] = Array();
     arr_atleta_foto_topo[1][0] = 'leandrinho_top.png';
     arr_atleta_foto_base[1] = Array();
     arr_atleta_foto_base[1][0] = 'leandrinho_botom.png';
          arr_atleta_nome[2] = 'Leco';
     arr_atleta_foto_topo[2] = Array();
     arr_atleta_foto_topo[2][0] = 'leco_top.png';
     arr_atleta_foto_base[2] = Array();
     arr_atleta_foto_base[2][0] = 'leco_botom.png';
          arr_atleta_nome[3] = 'Ricardinho';
     arr_atleta_foto_topo[3] = Array();
     arr_atleta_foto_topo[3][0] = 'ricardinho_top.png';
     arr_atleta_foto_base[3] = Array();
     arr_atleta_foto_base[3][0] = 'ricardinho_botom.png';
          arr_atleta_nome[4] = 'Thiago Carioca';
     arr_atleta_foto_topo[4] = Array();
     arr_atleta_foto_topo[4][0] = 'thiago_carioca_top.png';
     arr_atleta_foto_base[4] = Array();
     arr_atleta_foto_base[4][0] = 'thiago_carioca_botom.png';
          arr_atleta_nome[5] = 'Tiago';
     arr_atleta_foto_topo[5] = Array();
     arr_atleta_foto_topo[5][0] = 'tiago_top.png';
     arr_atleta_foto_base[5] = Array();
     arr_atleta_foto_base[5][0] = 'tiago_botom.png';
          arr_atleta_nome[6] = 'Vander Carioca';
     arr_atleta_foto_topo[6] = Array();
     arr_atleta_foto_topo[6][0] = 'wander_carioca_top.png';
     arr_atleta_foto_base[6] = Array();
     arr_atleta_foto_base[6][0] = 'wander_carioca_botom.png';
       var diretorio   = "arquivos/jogadores/jogadores_destaque/";
  var trocou_topo  = false;
  var trocou_base  = false;
  var intRandomMain  = 0;
  var intRandomSub = 0;
  var controle_loop  = 0;
  var str_img_topo = "";
  var str_img_bottom = "";
  //troca topo
  while( (!trocou_topo) && (controle_loop < 200) ){
   intRandomMain = rand(1,7) - 1;
   if(arr_atleta_foto_topo[intRandomMain].length > 0){
