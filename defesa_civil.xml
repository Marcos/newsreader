          return false;
          }
         
          if(!er.exec(sEmail)){
           alert('Email Invalido');
           return false;
         }   
         $.ajax({
          type: 'POST',
          url: $('#news-letter').attr('action'),
          data: 'mail='+sEmail+'&name='+nome,
          success: function(msg){
            alert( msg );
            $('#nome-mail').val('Nome');
            $('#e-mail').val('E-mail');
          }
         });             
         return false;
        });     
     });
